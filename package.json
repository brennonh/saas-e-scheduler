{
  "name": "saas-e-scheduler",
  "version": "0.0.1",
  "license": "SEE LICENSE IN ./LICENSE",
  "description": "A sassy scheduler",
  "main": "src/app.ts",
  "scripts": {
    "db.migrate": "yarn nps db.migrate",
    "db.revert": "yarn nps db.revert",
    "db.drop": "yarn nps db.drop",
    "db.seed": "yarn nps db.seed",
    "init-repo": "./kickstart.sh 1 && cp .env.example .env && yarn install",
    "sync-boilerplate": "./kickstart.sh 2",
    "nps": "cross-env NODE_ENV=development nps",
    "serve": "cross-env NODE_ENV=development nps serve",
    "test": "cross-env NODE_ENV=test nps test",
    "testAndWatch": "cross-env NODE_ENV=test nps 'test --watchAll'",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "build": "nps build"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express",
    "boilerplate"
  ],
  "dependencies": {
    "@google-cloud/debug-agent": "^5.1.2",
    "@google-cloud/pubsub": "^0.31.1",
    "axios": "^0.19.2",
    "chalk": "^2.4.1",
    "class-validator-jsonschema": "^1.3.0",
    "commander": "^2.19.0",
    "compression": "^1.7.1",
    "copyfiles": "^2.1.0",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dotenv": "6.0.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "figlet": "^1.2.0",
    "glob": "^7.1.4",
    "googleapis": "43.0.0",
    "helmet": "^3.20.0",
    "jsonfile": "5.0.0",
    "microframework-w3tec": "^0.6.3",
    "morgan": "^1.9.0",
    "mysql": "^2.17.1",
    "nodemon": "^1.12.1",
    "nps": "^5.9.3",
    "nps-utils": "^1.5.0",
    "pg": "^7.12.0",
    "reflect-metadata": "^0.1.10",
    "routing-controllers": "^0.7.7",
    "routing-controllers-openapi": "^1.7.0",
    "serve-favicon": "^2.4.5",
    "supertest": "^3.0.0",
    "swagger-stats": "^0.95.7",
    "swagger-ui-express": "4.0.7",
    "ts-node": "7.0.1",
    "tslint": "^5.8.0",
    "typedi": "0.8.0",
    "typeorm": "^0.2.18",
    "typeorm-seeding": "1.0.0-beta.7",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "3.2.2",
    "uuid": "^3.3.2",
    "winston": "3.2.1"
  },
  "resolutions": {
    "**/event-stream": "^4.0.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "rootDir": ".",
    "testRegex": "./test/.*test.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./src/utils/test/setup.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/bluebird": "^3.5.18",
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.11.0",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/faker": "^4.1.2",
    "@types/figlet": "^1.2.0",
    "@types/helmet": "0.0.41",
    "@types/jest": "^23.3.2",
    "@types/morgan": "^1.7.35",
    "@types/nock": "^9.1.3",
    "@types/reflect-metadata": "0.1.0",
    "@types/serve-favicon": "^2.2.29",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "cross-env": "^5.1.1",
    "jest": "29.3.1",
    "mock-express-request": "^0.2.0",
    "mock-express-response": "^0.2.1",
    "ncp": "^2.0.0",
    "nock": "10.0.0",
    "pubsub-js": "1.7.0",
    "sqlite3": "^4.0.9",
    "ts-jest": "^23.10.1"
  }
}
